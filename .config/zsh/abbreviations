# Dotfiles
abbr dfa="df add"
abbr dfc="df commit -m"
abbr dfp="df push"
abbr dfs="df status"

# Git
abbr gl="git --no-pager log --oneline -15"
abbr gg="git log --graph --decorate --oneline --all"
abbr gs="git status"

# Watson
abbr wl="watson log"
abbr we="watson edit"
abbr wa="watson add --from '' --to ''"
